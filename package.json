{
  "name": "yeser",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev-client:android": "EXPO_PUBLIC_ENV=development expo run:android",
    "dev-client:ios": "EXPO_PUBLIC_ENV=development expo run:ios",
    "preview-client:android": "EXPO_PUBLIC_ENV=preview expo run:android",
    "preview-client:ios": "EXPO_PUBLIC_ENV=preview expo run:ios",
    "lint": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,json,md}",
    "prepare": "husky || true",
    "postinstall": "if [ \"$CI\" != \"true\" ]; then husky; fi",
    "type-check": "tsc --noEmit --skipLibCheck",
    "audit:security": "npm audit --audit-level=moderate",
    "start:dev": "EXPO_PUBLIC_ENV=development expo start --clear",
    "start:preview": "EXPO_PUBLIC_ENV=preview expo start --clear",
    "start:prod": "EXPO_PUBLIC_ENV=production expo start --clear",
    "build:dev": "eas build --platform all --profile development --local",
    "build:dev:android": "eas build --platform android --profile development --local",
    "build:dev:ios": "eas build --platform ios --profile development --local",
    "build:preview": "eas build --platform all --profile preview --non-interactive",
    "build:preview:android": "eas build --platform android --profile preview --non-interactive",
    "build:preview:ios": "eas build --platform ios --profile preview --non-interactive",
    "build:production": "eas build --platform all --profile production --non-interactive",
    "build:production:android": "eas build --platform android --profile production --non-interactive",
    "build:production:ios": "eas build --platform ios --profile production --non-interactive",
    "build:production:aab": "eas build --platform android --profile production-aab --non-interactive",
    "submit:preview": "eas submit --platform all --profile preview --non-interactive",
    "submit:production": "eas submit --platform all --profile production --non-interactive",
    "update:preview": "eas update --branch preview --message",
    "update:production": "eas update --branch production --message",
    "validate:env": "node -e \"require('./src/utils/config.ts').logConfigurationStatus()\"",
    "validate:google-services": "node scripts/verify-google-services.js",
    "validate:build": "node scripts/validate-build.js && npm run type-check && npm run lint:check",
    "prebuild:preview": "npm run validate:build",
    "prebuild:production": "npm run validate:build",
    "lint:fix": "eslint src/ --fix",
    "lint:check": "eslint src/ --max-warnings=56"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@react-navigation/stack": "^7.3.3",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.80.2",
    "@tanstack/react-query-devtools": "^5.80.6",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^4.1.0",
    "expo": "53.0.18",
    "expo-auth-session": "5.5.2",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "^17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "^18.1.11",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "^16.1.6",
    "expo-notifications": "^0.31.4",
    "expo-print": "~14.1.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.16",
    "expo-web-browser": "^14.2.0",
    "lottie-react-native": "^7.2.2",
    "moti": "^0.30.0",
    "pako": "^2.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1312.1",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-tcp-socket": "^6.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "readable-stream": "^4.5.2",
    "stream-http": "^3.2.0",
    "toggle-switch-react-native": "^3.3.0",
    "url-lite": "^1.0.2",
    "zod": "^3.25.48",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.32.0",
    "@expo/config-plugins": "^10.1.2",
    "@types/react": "~19.0.10",
    "@types/react-native-calendars": "^1.1264.7",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob": "10.4.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "overrides": {
    "glob": "10.4.5",
    "brace-expansion": "2.0.1",
    "minimatch": "9.0.5"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "base64-arraybuffer",
          "pako",
          "readable-stream",
          "stream-http",
          "url-lite",
          "react-native-tcp-socket",
          "react-native-vector-icons",
          "toggle-switch-react-native"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
